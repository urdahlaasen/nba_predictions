CREATE TABLE group (
  gid int PRIMARY KEY auto_increment,
  groupname VARCHAR(255),
  memberNO int,
);

CREATE TABLE users (
  uid int PRIMARY KEY auto_increment,
  username VARCHAR(40),
  password VARCHAR(255),
  groupId INT,
  FOREIGN KEY (groupId) REFERENCES group(GID)
);

CREATE TABLE teams (
  teamid int PRIMARY KEY auto_increment,
  teamname VARCHAR(50),
  conference int,
  division int,
  arena VARCHAR(50),
  primarycolor VARCHAR(30),
  secondarycolor VARCHAR(30)
  );

